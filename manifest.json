{
   "author": "Eleazar Levchenko",
   "background": {
      "scripts": [ "main.js" ]
   },
   "browser_action": {
      "default_icon": {
	  "16": "images/16.png",
	  "32": "images/32.png"
       },
       "default_title": "Google Books Right-Click Search"
   },
   "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "64": "images/64.png",
      "128": "images/128.png"
   },
   "manifest_version": 2,
   "name": "Google Books Right-Click Search",
   "short_name": "GB Search",
   "description": "Search for selected text on Google Books via right-click and see the results in a new 'passive' tab",
   "permissions": [ "contextMenus", "tabs", "http://*/*", "https://*/*" ],
   "version": "1.0.0"
}
